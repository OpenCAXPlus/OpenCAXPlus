puts ""
#################################################################
## Exception during "tcopy" command
#################################################################

restore [locate_data_file OCC298.brep] result
checkshape result

if [catch {tcopy result a } res] {
  puts "Faulty OCC298: function TCOPY works wrongly"
} else {
  puts "OCC298 OK: function TCOPY works properly"
}

set square 8e+100
set 2dviewer 0

