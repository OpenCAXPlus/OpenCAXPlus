#################################
## Problems with the results of MakePipeShell
#################################

vinit
vsetdispmode 1
OCC544
vdisplay result
vsetmaterial result PLASTIC
vsetcolor result MATRABLUE
vsettransparency result 0.5
vsetdispmode 1
vfit

set scale    2.05374
set proj_X   0.135192
set proj_Y   -0.978297
set proj_Z   -0.157031
set up_X     -0.399854
set up_Y     -0.198875
set up_Z     0.894743
set at_X     415.781529476262
set at_Y     349.647084890243
set at_Z     417.026634136105

vviewparams -scale ${scale} -proj ${proj_X} ${proj_Y} ${proj_Z} -up ${up_X} ${up_Y} ${up_Z} -at ${at_X} ${at_Y} ${at_Z}

set x1 300
set y1 70

set x2 190
set y2 110

set x3 220
set y3 110

vmoveto ${x1} ${y1}

checkcolor $x3 $y3 0.04 0.04 0.5

set only_screen 1
