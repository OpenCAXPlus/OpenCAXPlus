pload QAcommands

puts "======="
puts "OCC24"
puts "======="
puts ""
###########################################################################
## Recorded modifications are lost when converting from ShapeBuild_ReShape into  ShapeProcess_ShapeContext
## and back
###########################################################################

restore [locate_data_file OCC24.brep] s
checkshape s

explode s f

compound s_1 c
if [catch { OCC24 result s c OCC24 Seq } ] {
  puts "OCC24 ERROR"
} else {
  puts "OCC24 OK"
}

set square 37.6991
set 2dviewer 2
