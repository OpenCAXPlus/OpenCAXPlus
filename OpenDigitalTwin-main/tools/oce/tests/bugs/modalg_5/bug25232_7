puts "============"
puts "OCC25232"
puts "============"
puts ""
#########################################################################
# Functionality to create solids from set of shapes
#########################################################################

cylinder cyl 10
trimv cyl cyl -20 20
mkface fcyl cyl
cone con 45 0
trimv con1 con 0 15
trimv con2 con -15 0
mkface fcon1 con1
mkface fcon2 con2
plane pl 0 0 15 0 0 1
mkface fp pl -15 15 -15 15
sphere sph 0 0 0 12
mkface fsph sph

mkvolume result fcyl fp
# 0 solids

set nb_v_good 0
set nb_e_good 0
set nb_w_good 0
set nb_f_good 0
set nb_sh_good 0
set nb_sol_good 0
set nb_compsol_good 0
set nb_compound_good 1
set nb_shape_good 1

set 2dviewer 1
