#########################################################################
# Functionality to create solids from set of shapes
#########################################################################

box b1 10 10 10
box b2 4 9 4 2 2 2
explode b2 w
sphere s 5 5 5 5
mkface f s
mkvolume result b1 f b2_1
# 2 solids created

set square 1228.32

set nb_v_good 20
set nb_e_good 21
set nb_w_good 7
set nb_f_good 7
set nb_sh_good 3
set nb_sol_good 2
set nb_compsol_good 0
set nb_compound_good 1
set nb_shape_good 61

set 2dviewer 1
