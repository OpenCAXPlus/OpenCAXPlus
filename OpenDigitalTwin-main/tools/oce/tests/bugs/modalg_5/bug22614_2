puts ""
#######################################################################
# Scaling fails with linear (p-)curves
#######################################################################

pcylinder p 10 50

explode p f

# p_2 is top planar face
tscale p_2 0 0 0 0.001

renamevar p_2 result

set square 0.000314159

set nb_v_good 1
set nb_e_good 1
set nb_w_good 1
set nb_f_good 1
set nb_sh_good 0
set nb_sol_good 0
set nb_compsol_good 0
set nb_compound_good  0
set nb_shape_good 4

set 2dviewer 1
