puts "========================"
puts "OCC22614"
puts "========================" 
puts ""
#######################################################################
# Scaling fails with linear (p-)curves
#######################################################################

pcylinder p 10 50

explode p f

# p_1 is a cylindrical face
tscale p_1 0 0 0 0.001

renamevar p_1 result

set square 0.00314159

set nb_v_good 2
set nb_e_good 3
set nb_w_good 1
set nb_f_good 1
set nb_sh_good 0
set nb_sol_good 0
set nb_compsol_good 0
set nb_compound_good  0
set nb_shape_good 7

set 2dviewer 1
