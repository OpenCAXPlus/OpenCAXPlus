
puts "========================"
puts "BUC61027"
puts " OCC368 "
puts "========================"
puts ""
##############################################
## Visualization is too slow.
##############################################

restore [locate_data_file OCC368.brep] result
checkshape result

tclean result
isos result 0
vinit
dchrono h2 reset
dchrono h2 start
vdisplay result
vsetdispmode result 1
dchrono h2 stop

regexp {CPU user time: +([-0-9.+eE]+)} [dchrono h2 show] full z2

if { $z2 > 30 } {                             
   puts "Error : Elapsed time is more then 30 seconds"
}

set 2dviewer 0




