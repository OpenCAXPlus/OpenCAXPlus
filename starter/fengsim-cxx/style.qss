QToolTip
{
    border: 1px solid #76797C;
    background-color: rgb(90, 102, 117);
    color: white;
    padding: 2px;
    opacity: 200;
    font-size: 9pt;
}

QWidget
{
    background: #31363b;
    color: #eff0f1;
    selection-background-color:white;
    selection-color: #eff0f1;
    background-clip: border;
    border-image: none;
    border: 2px transparent;
    outline: 2;
}

QWidget:item:hover
{
    background-color: #18465d;
    color: #eff0f1;
}

QWidget:item:unselected
{
    background-color: transparent;
    color: #eff0f1;
}

QWidget:item:selected
{
    /*background-color: #18465d;*/
    background-color: transparent;
}

QWidget:item:checked
{
     /*font-weight: bold;*/
     background-color: transparent;

}

QCheckBox
{
    spacing: 5px;
    outline: 2px;
    color: white;
    margin-bottom: 2px;
}

QCheckBox:disabled
{
    color: #76797C;
}

QCheckBox::indicator, QGroupBox::indicator
{
    width: 16px;
    height: 16px;
}
QGroupBox::indicator
{
    margin-left: 4px;
}

QCheckBox::indicator:unchecked
{
    image: url(:/figure/fem_wind/unchecked.png);
    background-color: transparent;
}

QCheckBox::indicator:unchecked:hover,
QCheckBox::indicator:unchecked:focus,
QCheckBox::indicator:unchecked:pressed,
QGroupBox::indicator:unchecked:hover,
QGroupBox::indicator:unchecked:focus,
QGroupBox::indicator:unchecked:pressed
{
    image: url(:/figure/fem_wind/unchecked.png);
    background-color: transparent;
}

QCheckBox::indicator:checked
{
    image: url(:/figure/fem_wind/checked.png);
    background-color: transparent;
}

QCheckBox::indicator:checked:hover,
QCheckBox::indicator:checked:focus,
QCheckBox::indicator:checked:pressed,
QGroupBox::indicator:checked:hover,
QGroupBox::indicator:checked:focus,
QGroupBox::indicator:checked:pressed
{
  background-color: transparent;
  image: url(:/figure/fem_wind/checked.png);
}


QCheckBox::indicator:indeterminate
{
    image: url(:/machining_wind/figure/machining_wind/uncheck_png);
}

QCheckBox::indicator:indeterminate:focus,
QCheckBox::indicator:indeterminate:hover,
QCheckBox::indicator:indeterminate:pressed
{
    image: url(:/machining_wind/figure/machining_wind/check.png);
}

QCheckBox::indicator:checked:disabled,
QGroupBox::indicator:checked:disabled
{
    image: none;
}

QCheckBox::indicator:unchecked:disabled,
QGroupBox::indicator:unchecked:disabled
{
    image: none;
}

QRadioButton
{
    spacing: 5px;
    outline: none;
    color: #eff0f1;
    margin-bottom: 2px;
}

QRadioButton:disabled
{
    color: #76797C;
}
QRadioButton::indicator
{
    width: 21px;
    height: 21px;
}

QRadioButton::indicator:unchecked
{
    image: none;
}


QRadioButton::indicator:unchecked:hover,
QRadioButton::indicator:unchecked:focus,
QRadioButton::indicator:unchecked:pressed
{
    border: none;
    outline: none;
    image: none;
}

QRadioButton::indicator:checked
{
    border: none;
    outline: none;
    image: none;
}

QRadioButton::indicator:checked:hover,
QRadioButton::indicator:checked:focus,
QRadioButton::indicator:checked:pressed
{
    border: none;
    outline: none;
    image: none;
}

QRadioButton::indicator:checked:disabled
{
    outline: none;
    image: none;
}

QRadioButton::indicator:unchecked:disabled
{
    image: url(:/qss_icons/rc/radio_unchecked_disabled.png);
}

QDialog, QFileDialog {
    background-color: #31363b;
}

QMenuBar
{
    background: 1px solid rgb(33,40,48);
    color: #000000;
    border: 0px solid #6A8480;
    selection-color: white;
    margin: 0px;
}

QMenuBar::selected
{
    background: transparent;
    border: 1px solid #6A8480;
    background-color: #6A8480;
    color: #000000;
}

QMenuBar::item
{
    background: transparent;
    color: white;
}



QMenuBar::item:selected
{
    background: transparent;
    border: 0px solid #6A8480;
    background-color: rgb(67,67,67);
    color: white;
}

QMenuBar::item:pressed
{
    border: 0px solid #6A8480;
    background-color: rgb(67,67,67);
    color: white;
}

QMenu
{
    border: transparent;
    color: #000000;
    /*margin: 3px;*/
    border: 2px solid rgb(158,158,158);
}

QMenu::selected
{
    background: transparent;
    border: transparent;
    background-color: #6A8480;
    color: #6A8480;
}

QMenu::pressed
{
    background: transparent;
    border: transparent;
    background-color: #6A8480;
    color: #6A8480;
}

QMenu::hover
{
    background: transparent;
    border: 2px solid white;
    background-color: rgb(158,158,158);
    color: #6A8480;
}

QMenu::icon
{
    margin: 0px;
}

QMenu::item
{
    font-size: 9pt;
    padding: 3px 1px 1px 3px;
    margin: 3px;
    margin-right: 10px;
    border: 1px solid transparent; /* reserve space for selection border */
    color: white;
}

QMenu::item:selected
{
    background-color: transparent;
    color: #6A8480;
    text-decoration:underline;
    font-size: 10pt;
}

QMenu::item:pressed
{
    background: transparent;
    border: transparent;
    background-color: transparent;
    color: #6A8480;
    font-size: 10pt;
}

QMenu::item::checked, QMenu::item::hover
{
    font-size: 10pt;
    background: transparent;
    color: #6A8480;
}

QMenu::separator {
    height: 2px;
    background: white;
    margin-left: 10px;
    margin-right: 5px;
}

QMenu::indicator {
    width: 5px;
    height: 5px;
}

